{
  "name": "@internal/met.no",
  "version": "0.0.1",
  "description": "OpenAPI client for @internal/met.no",
  "author": "OpenAPI-Generator Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/GIT_USER_ID/GIT_REPO_ID.git"
  },
  "keywords": [
    "axios",
    "typescript",
    "openapi-client",
    "openapi-generator",
    "@internal/met.no"
  ],
  "license": "Unlicense",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "module": "./dist/esm/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "prepare": "npm run build",
    "gen-if-needed": "if ! test -d ./dist; then yarn generate-openapi; else echo 'Not regenerating client: already exists.'; fi",
    "generate-openapi": "openapi-generator-cli generate && yarn install && yarn build"
  },
  "dependencies": {
    "axios": "^1.6.8"
  },
  "devDependencies": {
    "@types/node": "^20.12.6",
    "typescript": "^5.4"
  }
