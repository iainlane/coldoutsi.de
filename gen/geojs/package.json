{
    "name": "@internal/geojs",
    "version": "0.0.1",
    "description": "OpenAPI client for @internal/geojs",
    "author": "OpenAPI-Generator Contributors",
    "repository": {
        "type": "git",
        "url": "https://github.com/GIT_USER_ID/GIT_REPO_ID.git"
    },
    "keywords": [
        "axios",
        "typescript",
        "openapi-client",
        "openapi-generator",
        "@internal/geojs"
    ],
    "license": "Unlicense",
    "main": "./dist/index.js",
    "typings": "./dist/index.d.ts",
    "module": "./dist/esm/index.js",
    "sideEffects": false,
    "scripts": {
        "build": "tsc && tsc -p tsconfig.esm.json",
        "prepare": "npm run build",
        "gen-if-needed": "/bin/sh -c 'if ! test -d ./dist; then pnpm generate-openapi; else echo \"Not regenerating client: already exists.\"; fi'",
        "generate-openapi": "pwd && openapi-generator-cli generate && pnpm install && pnpm build"
    },
    "dependencies": {
        "axios": "^1.7.9"
    },
    "devDependencies": {
        "@openapitools/openapi-generator-cli": "^2.16.2",
        "@types/node": "^22.10.7",
        "typescript": "^5.7.3"
    }
}
