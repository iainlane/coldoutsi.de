{
    "name": "@internal/geojs",
    "version": "0.0.1",
    "description": "OpenAPI client for @internal/geojs",
    "author": "OpenAPI-Generator Contributors",
    "repository": {
        "type": "git",
        "url": "https://github.com/GIT_USER_ID/GIT_REPO_ID.git"
    },
    "keywords": [
        "axios",
        "typescript",
        "openapi-client",
        "openapi-generator",
        "@internal/geojs"
    ],
    "license": "Unlicense",
    "main": "./dist/index.js",
    "typings": "./dist/index.d.ts",
    "module": "./dist/esm/index.js",
    "sideEffects": false,
    "scripts": {
        "build": "tsc && tsc -p tsconfig.esm.json",
        "prepare": "npm run build",
        "gen-if-needed": "if ! test -d ./dist; then yarn generate-openapi; else echo 'Not regenerating client: already exists.'; fi",
        "generate-openapi": "openapi-generator-cli generate && yarn install && yarn build"
    },
    "dependencies": {
        "axios": "^1.6.1"
    },
    "devDependencies": {
        "@openapitools/openapi-generator-cli": "^2.7.0",
        "@types/node": "^12.11.5",
        "typescript": "^4.0"
    }
}
